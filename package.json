{
  "name": "duplifolder",
  "author": "TannerOrdonez",
  "publisher": "TannerOrdonez",
  "displayName": "Duplifolder",
  "description": "",
  "version": "4.1.0",
  "icon": "resources/icon1.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "icons": {
    "default": "resources/icon1.png"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:duplifolder.activate",
    "onCommand:vscode.open",
    "onCommand:workbench.action.openFolder",
    "onCommand:workbench.action.closeFolder"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "duplifolder-bar",
          "title": "Duplifolder",
          "icon": "resources/icon1.png"
        }
      ]
    },
    "views": {
      "duplifolder-bar": [
        {
          "icon": "resources/icon1.png",
          "id": "backupsection",
          "name": "Options"
        },
        {
          "icon": "resources/icon1.png",
          "id": "backuphistory",
          "name": "History"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "os": "^0.1.2",
    "path": "^0.12.7"
  }
}
